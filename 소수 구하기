num = 1

while num < 101 :
  # 2의 배수 
  if num % 2 == 0:
    if num == 2 :
      print(num, " = 소수")
    elif num % 3 == 0:
      if num == 3:
        print(num, " = 소수")
      else :
        print(num, " = 2와 3의 배수")
    else:
      print(num, " = 2의 배수")
  elif num % 3 == 0 :
    if num == 3:
      print(num, " = 소수")
    else:
      print(num, " = 3의 배수")
  else :
    print(num, " = 소수")
  num += 1 
